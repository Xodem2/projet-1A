<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AfterGameWaitingRoomActivity"
    >

    <VideoView
        android:id="@+id/backend"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />
    <TextView
        android:id="@+id/afterGameWaitingRoomStatusFillId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="100dp"
        android:text="TextView"
        android:textColor="#000000"
        android:textSize="40dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/afterGameWaitingRoomPlayer1FillId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        android:text="p1"
        android:textColor="#000000"
        android:textSize="25dp"
        app:layout_constraintEnd_toStartOf="@+id/afterGameWaitingRoomP2ScoreFillId"
        app:layout_constraintHorizontal_bias="0.429"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/afterGameWaitingRoomStatusFillId" />

    <TextView
        android:id="@+id/afterGameWaitingRoomP1ScoreFillId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="56dp"
        android:text="p1Score"
        android:textColor="#000000"
        android:textSize="25dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/afterGameWaitingRoomStatusFillId" />

    <TextView
        android:id="@+id/afterGameWaitingRoomPlayer2FillId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="48dp"
        android:text="p2"
        android:textColor="#000000"
        android:textSize="25dp"
        app:layout_constraintEnd_toStartOf="@+id/afterGameWaitingRoomP2ScoreFillId"
        app:layout_constraintHorizontal_bias="0.428"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/afterGameWaitingRoomPlayer1FillId" />

    <TextView
        android:id="@+id/afterGameWaitingRoomP2ScoreFillId"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="48dp"
        android:layout_marginEnd="56dp"
        android:text="p2Score"
        android:textColor="#000000"
        android:textSize="25dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/afterGameWaitingRoomP1ScoreFillId" />

    <ImageButton
        android:id="@+id/afterGameWaitingRoomQuitButtonId"
        android:layout_width="144dp"
        android:layout_height="76dp"
        android:background="@null"
        android:scaleType="fitCenter"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/afterGameWaitingRoomPlayer2FillId"
        app:layout_constraintVertical_bias="0.792"
        app:srcCompat="@drawable/button_quitter" />
</androidx.constraintlayout.widget.ConstraintLayout>